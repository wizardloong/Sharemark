{
  "manifest_version": 3,
  "name": "ShareMark",
  "version": "0.1",
  "description": "Share bookmark folders with others.",
  "permissions": ["bookmarks", "contextMenus", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "ShareMark"
  },
  "icons": {
    "48": "icon.png"
  }
}
